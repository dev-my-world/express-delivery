<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="dao.UserDao">
    <insert id="addUser" parameterType="model.User">
        INSERT INTO user_info (user_phone, user_psw) VALUES (#{userPhone}, #{userPsw})
    </insert>


    <select id="getUser" resultType="model.User" parameterType="model.User">
        SELECT *
        FROM user_info
        WHERE user_info.user_phone = #{userPhone};
    </select>
</mapper>